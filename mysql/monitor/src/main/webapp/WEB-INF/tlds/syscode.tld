	<tag>
		<name>dictionary</name>
		<tag-class>com.fintech.platform.restservice.web.tag.SysCodeTag</tag-class>
		<body-content>JSP</body-content>
		<attribute>
			<name>id</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
